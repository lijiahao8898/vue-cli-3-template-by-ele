{
  "name": "vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "element-ui": "^2.12.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "shared-node-browser": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-tabs": "off",
      "comma-dangle": 0,
      "no-extra-semi": 0,
      "new-cap": 0,
      "no-new": 0,
      "no-undef": 0,
      "max-len": 0,
      "no-useless-escape": 0,
      "space-before-function-paren": [
        0,
        "always"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-console": 0,
      "generator-star-spacing": "off",
      "no-unused-expressions": "off",
      "vue/script-indent": [
        "error",
        2,
        {
          "baseIndent": 1
        }
      ],
      "overrides": [
        {
          "files": [
            "*.vue"
          ],
          "rules": {
            "indent": "off"
          }
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
